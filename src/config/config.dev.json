{
    "auth": {
        "secret": "test123",
        "algorithm": "HS256",
        "verifyOptions": {
            "algorithms": [
                "HS256"
            ]
        }
    },
    "server": {
        "host": "localhost",
        "port": 8080,
        "plugins": [
            "swagger"
        ]
    },
    "database": {
        "host": "localhost",
        "port": 5432,
        "name": "mrgigabyte",
        "username": "mrgigabyte",
        "password": "abc@123",
        "pool": {
            "min": 0,
            "max": 10,
            "acquire": 30000,
            "idle": 1000
        },
        "dialect": "postgres"
    },
    "plugins": {
        "hapi-swagger": {
            "info": {
                "title": "hapi-boilerplate",
                "version": "1.0.0"
            },
            "securityDefinitions": {
                "jwt": {
                    "type": "apiKey",
                    "name": "Authorization",
                    "in": "header"
                }
            }
        }
    }
}